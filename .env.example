# Infura API key — constructs coordinator RPC URLs (Sepolia / Ethereum)
INFURA_API_KEY=your-infura-api-key

# Signing chain RPC — Base Sepolia (devnet) or Base (mainnet)
SIGNING_CHAIN_RPC_URL=https://your-base-sepolia-rpc-url

# Pimlico API key — ERC-4337 bundler/paymaster
PIMLICO_API_KEY=your-pimlico-api-key

# Discord webhook for daily reports (optional, used by CI)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/...
